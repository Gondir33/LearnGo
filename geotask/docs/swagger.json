                "location": {
                    "$ref": "#/definitions/models.Point"
                },
                "score": {
                    "type": "integer"
                }
            }
        },
        "models.CourierStatus": {
            "type": "object",
            "properties": {
                "courier": {
                    "$ref": "#/definitions/models.Courier"
                },
                "orders": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/models.Order"
                    }
                }
            }
        },
        "models.Order": {
            "type": "object",
            "properties": {
                "created_at": {
                    "type": "string"
                },
                "delivery_price": {
                    "type": "number"
                },
                "id": {
                    "type": "integer"
                },
                "is_delivered": {
                    "type": "boolean"
                },
                "lat": {
                    "type": "number"
                },
                "lng": {
                    "type": "number"
                },
                "price": {
                    "type": "number"
                }
            }
        },
        "models.Point": {
            "type": "object",
            "properties": {
                "lat": {
                    "type": "number"
                },
                "lng": {
                    "type": "number"
                }
            }
        }
    }
