{
    "swagger": "2.0",
    "info": {
        "description": "API Server for HugoMap Application",
        "title": "HugoMap",
        "contact": {},
        "version": "1.0"
    },
    "host": "localhost:8080",
    "basePath": "/",
    "paths": {
        "/api/status": {
            "get": {
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "status"
                ],
                "summary": "Получение статуса сервиса",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/models.CourierStatus"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "models.Courier": {
            "type": "object",
